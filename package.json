{
	"name": "vscode-dragonbones",
	"displayName": "vscode-dragonbones",
	"description": "vscode extension for preview dragonboens zip file",
	"icon": "icon.png",
	"version": "0.1.1",
	"publisher": "gnoijli",
	"engines": {
		"vscode": "^1.56.0"
	},
	"categories": [
		"Other"
	],
	"repository": {},
	"activationEvents": [
		"onCommand:vsc-extension.openDBPreview"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vsc-extension.openDBPreview",
				"title": "open dragonbones preview"
			}
		],
		"languages": [
			{
				"id": "dbz",
				"aliases": [
					"DBZ"
				],
				"extensions": [
					".dbz",
					".zip",
					".bin"
				]
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "vsc-extension.openDBPreview",
					"when": "resourceLangId == dbz",
					"group": "dbPreview@1"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"package": "rimraf ./dist && mkdirp ./dist && vsce package -o ./dist/"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "^15.3.0",
		"@types/vscode": "^1.56.0",
		"@typescript-eslint/eslint-plugin": "^4.24.0",
		"@typescript-eslint/parser": "^4.24.0",
		"eslint": "^7.26.0",
		"glob": "^7.1.7",
		"mkdirp": "^1.0.4",
		"mocha": "^8.4.0",
		"rimraf": "^3.0.2",
		"typescript": "^4.2.4",
		"vsce": "^1.88.0",
		"vscode-test": "^1.5.2"
	}
}
